@article{silber_comparing_1995,
	title = {Comparing the Contributions of Groups of Predictors: Which Outcomes Vary With Hospital Rather Than Patient Characteristics},
	volume = {90},
	issn = {0162-1459},
	url = {http://www.jstor.org/stable/2291124},
	doi = {10.2307/2291124},
	shorttitle = {Comparing the Contributions of Groups of Predictors},
	abstract = {In a model, such as a logit regression model, we wish to compare the relative importance of two groups of predictors for various objectives. In the example that motivated this work, the model predicts patient outcomes during hospital stays, and we wish to measure the relative contribution of patient and hospital characteristics to the variation in outcomes among patients and among hospitals. This is done using the relative dispersion of patient and hospital contributions to the fitted outcomes. As is seen, this question is distinct from other common questions, including the quality of the overall fit, the degree to which the outcome is accurately predicted, the statistical significance of groups of predictors, and the correlations among and between groups of predictors. Relevant point estimates, confidence intervals, and hypothesis tests are developed. In the example, we examine three outcome measures and find that nearly all of the predictable variation in patient outcomes and most of the predictable variation in outcomes among hospitals reflects variation in patient characteristics rather than hospital characteristics; however, this is true in varying degrees for the three outcomes.},
	pages = {7--18},
	number = {429},
	journaltitle = {Journal of the American Statistical Association},
	author = {Silber, Jeffrey H. and Rosenbaum, Paul R. and Ross, Richard N.},
	urldate = {2021-01-04},
	date = {1995},
	note = {Publisher: [American Statistical Association, Taylor \& Francis, Ltd.]},
	file = {Silber et al_1995_Comparing the Contributions of Groups of Predictors.pdf:C\:\\Users\\nh1087\\Zotero\\storage\\P8FI3X6S\\Silber et al_1995_Comparing the Contributions of Groups of Predictors.pdf:application/pdf}
}

