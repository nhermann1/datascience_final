	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1046/j.1466-822X.2003.00042.x},
	keywords = {climate change, ecological modelling, ecological niche, bioclimate envelope, climate space, hierarchy, scale},
	file = {Snapshot:C\:\\Users\\nh1087\\Zotero\\storage\\9YQHYY65\\j.1466-822X.2003.00042.html:text/html;Pearson_Dawson_2003_Predicting the impacts of climate change on the distribution of species.pdf:C\:\\Users\\nh1087\\Zotero\\storage\\ZK97Z3RZ\\Pearson_Dawson_2003_Predicting the impacts of climate change on the distribution of species.pdf:application/pdf}
