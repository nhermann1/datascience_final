	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1600-0633.1999.tb00071.x},
	keywords = {Atlantic salmon, competition exclusive principle, resource partitioning, Siberian sculpin},
	file = {Snapshot:C\:\\Users\\nh1087\\Zotero\\storage\\SED6XMRX\\j.1600-0633.1999.tb00071.html:text/html;Gabler_Amundsen_1999_Resource partitioning between Siberian sculpin (Cottus poecilopus Heckel) and.pdf:C\:\\Users\\nh1087\\Zotero\\storage\\9L4DDEG5\\Gabler_Amundsen_1999_Resource partitioning between Siberian sculpin (Cottus poecilopus Heckel) and.pdf:application/pdf}
