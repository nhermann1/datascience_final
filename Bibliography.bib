@article{isaac_prey_2012,
	title = {Prey selection by the Lake Superior fish community},
	volume = {38},
	url = {http://dx.doi.org/10.1016/j.jglr.2012.02.017},
	doi = {10.1016/j.jglr.2012.02.017},
	abstract = {Mysis diluviana is an important prey item to the Lake Superior fish community as found through a recent diet study. We further evaluated this by relating the quantity of prey found in fish diets to the quantity of prey available to fish, providing insight into feeding behavior and prey preferences. We describe the seasonal prey selection of major fish species collected across 18 stations in Lake Superior in spring, summer, and fall of 2005. Of the major nearshore fish species, bloater (. Coregonus hoyi), rainbow smelt (. Osmerus mordax), and lake whitefish (. Coregonus clupeaformis) consumed . Mysis, and strongly selected . Mysis over other prey items each season. However, lake whitefish also selected . Bythotrephes in the fall when . Bythotrephes were numerous. Cisco (. Coregonus artedi), a major nearshore and offshore species, fed largely on calanoid copepods, and selected calanoid copepods (spring) and . Bythotrephes (summer and fall). Cisco also targeted prey similarly across bathymetric depths. Other major offshore fish species such as kiyi (. Coregonus kiyi) and deepwater sculpin (. Myoxocephalus thompsoni) fed largely on . Mysis, with kiyi targeting . Mysis exclusively while deepwater sculpin did not prefer any single prey organism. The major offshore predator siscowet lake trout (. Salvelinus namaycush siscowet) consumed deepwater sculpin and coregonines, but selected deepwater sculpin and . Mysis each season, with juveniles having a higher selection for . Mysis than adults. Our results suggest that . Mysis is not only a commonly consumed prey item, but a highly preferred prey item for pelagic, benthic, and piscivorous fishes in nearshore and offshore waters of Lake Superior. © 2012 Elsevier B.V.},
	pages = {326--335},
	number = {2},
	journaltitle = {Journal of Great Lakes Research},
	author = {Isaac, Edmund J. and Hrabik, Thomas R. and Stockwell, Jason D. and Gamble, Allison E.},
	date = {2012},
	note = {Publisher: Elsevier B.V.},
	keywords = {Prey selection, Fish community, Lake Superior, Food web, Mysis},
	file = {Isaac et al. - 2012 - Prey selection by the Lake Superior fish community:C\:\\Users\\nh1087\\Zotero\\storage\\MUHEEX6P\\Isaac et al. - 2012 - Prey selection by the Lake Superior fish community.pdf:application/pdf}
}

@article{higginson_foraging_2015,
	title = {Foraging mode switching: The importance of prey distribution and foraging currency},
	volume = {105},
	url = {http://dx.doi.org/10.1016/j.anbehav.2015.04.014},
	doi = {10.1016/j.anbehav.2015.04.014},
	abstract = {Foraging methods are highly variable, but can be grouped into two modes: searching and ambush. While research has focused on the functioning of each mode, the question of how animals choose which to use has been largely neglected. Here we consider a forager that exploits prey that are patchily distributed in space and time. This forager can either sit and wait for prey to appear or search for prey, which is more likely to result in encounters with prey but costs more energy and/or exposes the forager to greater predation risk. The currency that natural selection appears to have optimized will be determined by the additional costs of searching and whether there is a risk of starvation. We therefore compare the predictions of models based on currencies that consider only energy and predation risk to state-dependent models in which energy reserves are used to trade off predation rate, starvation rate and investment in growth. The choice of currency qualitatively affects how mode should change when prey abundance and prey patchiness increase. We show how differing prey distributions can explain variation in effects of experimentally increasing prey abundance. Our work has several implications for the study of foraging mode, population dynamics and the methods used to assess population size.},
	pages = {121--137},
	journaltitle = {Animal Behaviour},
	author = {Higginson, Andrew D. and Ruxton, Graeme D.},
	date = {2015},
	note = {Publisher: Elsevier Ltd},
	keywords = {Optimal foraging, Deep sea fish, Ectotherms, Marginal value theorem, Patch use, Prey distribution, Resource heterogeneity, Search strategy, Sit-and-wait},
	file = {Higginson, Ruxton - 2015 - Foraging mode switching The importance of prey distribution and foraging currency:C\:\\Users\\nh1087\\Zotero\\storage\\ZLYAT9SV\\Higginson, Ruxton - 2015 - Foraging mode switching The importance of prey distribution and foraging currency.pdf:application/pdf}
}

@article{huisman_food_1976,
