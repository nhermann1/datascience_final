	keywords = {climate change, biodiversity, climate envelopes, limited source, species–area curves, species–area prediction},
	file = {Snapshot:C\:\\Users\\nh1087\\Zotero\\storage\\4Z8SC9GB\\0012-9658(2006)87[1896PBCOTC]2.0.html:text/html;Ibáñez et al_2006_Predicting Biodiversity Change.pdf:C\:\\Users\\nh1087\\Zotero\\storage\\KM2WRATB\\Ibáñez et al_2006_Predicting Biodiversity Change.pdf:application/pdf}
