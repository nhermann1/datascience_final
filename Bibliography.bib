	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1365-2486.2008.01571.x},
	keywords = {climate change, zooplankton, phytoplankton, spring bloom},
	file = {Snapshot:C\:\\Users\\nh1087\\Zotero\\storage\\QBJLGF4Z\\j.1365-2486.2008.01571.html:text/html;Sommer_Lengfellner_2008_Climate change and the timing, magnitude, and composition of the phytoplankton.pdf:C\:\\Users\\nh1087\\Zotero\\storage\\VXM28MNV\\Sommer_Lengfellner_2008_Climate change and the timing, magnitude, and composition of the phytoplankton.pdf:application/pdf}
