@article{polyakov_weakening_2020,
	title = {Weakening of Cold Halocline Layer Exposes Sea Ice to Oceanic Heat in the Eastern Arctic Ocean},
	volume = {33},
	issn = {0894-8755, 1520-0442},
	url = {https://journals.ametsoc.org/doi/10.1175/JCLI-D-19-0976.1},
	doi = {10.1175/JCLI-D-19-0976.1},
	abstract = {Abstract
            
              A 15-yr duration record of mooring observations from the eastern ({\textgreater}70°E) Eurasian Basin ({EB}) of the Arctic Ocean is used to show and quantify the recently increased oceanic heat flux from intermediate-depth ({\textasciitilde}150–900 m) warm Atlantic Water ({AW}) to the surface mixed layer and sea ice. The upward release of {AW} heat is regulated by the stability of the overlying halocline, which we show has weakened substantially in recent years. Shoaling of the {AW} has also contributed, with observations in winter 2017–18 showing {AW} at only 80 m depth, just below the wintertime surface mixed layer, the shallowest in our mooring records. The weakening of the halocline for several months at this time implies that {AW} heat was linked to winter convection associated with brine rejection during sea ice formation. This resulted in a substantial increase of upward oceanic heat flux during the winter season, from an average of 3–4 W m
              −2
              in 2007–08 to {\textgreater}10 W m
              −2
              in 2016–18. This seasonal {AW} heat loss in the eastern {EB} is equivalent to a more than a twofold reduction of winter ice growth. These changes imply a positive feedback as reduced sea ice cover permits increased mixing, augmenting the summer-dominated ice-albedo feedback.},
	pages = {8107--8123},
	number = {18},
