	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1600-0633.2010.00461.x},
	keywords = {colonisation, colour change, glacial recession, Glacier Bay, phenotypic plasticity, sculpin},
	file = {Snapshot:C\:\\Users\\nh1087\\Zotero\\storage\\WMPSALA9\\j.1600-0633.2010.00461.html:text/html;Whiteley et al_2011_The spectre of past spectral conditions.pdf:C\:\\Users\\nh1087\\Zotero\\storage\\CYPZSSJ6\\Whiteley et al_2011_The spectre of past spectral conditions.pdf:application/pdf}
