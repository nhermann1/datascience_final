	langid = {english},
	file = {Scharf et al. - 2000 - Predator size-prey size relationships of marine fi.pdf:C\:\\Users\\nh1087\\Zotero\\storage\\AVHS57HS\\Scharf et al. - 2000 - Predator size-prey size relationships of marine fi.pdf:application/pdf}
}

