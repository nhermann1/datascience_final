}

@article{cederholm_pacific_1999,
	title = {Pacific Salmon Carcasses: Essential Contributions of Nutrients and Energy for Aquatic and Terrestrial Ecosystems},
	volume = {24},
	doi = {10.1577/1548-8446(1999)024<0006:psc>2.0.co;2},
	abstract = {Pacific salmon and other anadromous salmonids represent a major vector for transporting marine nutrients across ecosystem boundaries (i.e., from marine to freshwater and terrestrial ecosystems). Salmon carcasses provide nutrients and energy to biota within aquatic and terrestrial ecosystems through various pathways. In this paper we review and synthesize the growing number of studies documenting this process in different localities. We also discuss the implications for maintaining the nutrient feedback system. Our findings show that future management will need to view spawning salmon and their carcasses as important habitat components for sustaining the produc- tion of fish as well as other salmon-dependent species within watersheds. acific},
