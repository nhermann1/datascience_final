@article{hartman_bioenergetics_2017,
	title = {Bioenergetics of brown bullhead in a changing climate},
	volume = {146},
	doi = {10.1080/00028487.2017.1293563},
	abstract = {Brown Bullhead Ameiurus nebulosus is a benthic-oriented warmwater fish species distributed throughout the eastern United States and southeastern Canada, but widely released elsewhere. This study developed and validated a bioenergetics model for Brown Bullhead in the laboratory and then applied the model to evaluate potential changes in growth and distribution associated with climate change. The model performed very well in laboratory validations at two temperatures and two ration levels—within 4\% between observed and predicted values. Modeling of a 4°C increase in water temperatures due to climate warming suggested increases in growth in the northern parts of the species’ natural range and declines or extirpation in the south. In addition to distribution shifts, the warmer temperatures could increase biometabolism of polynuclear aromatic hydrocarbons ({PAHs}) or bioaccumulation at local scales that may lead to changes in population structure or consumption advisories of Brown Bullhead.},
	pages = {634--644},
	number = {4},
	journaltitle = {Transactions of the American Fisheries Society},
	author = {Hartman, Kyle J.},
	date = {2017},
	file = {Hartman-2017-Transactions_of_the_American_Fisheries_Society:C\:\\Users\\nh1087\\Zotero\\storage\\6CDG9S5N\\Hartman-2017-Transactions_of_the_American_Fisheries_Society.pdf:application/pdf}
}

@article{beauchamp_evaluation_1995,
	title = {Evaluation of Competition and Predation as Limits to Juvenile Kokanee and Sockeye Salmon Production in Lake Ozette, Washington},
	volume = {15},
	doi = {10.1577/1548-8675(1995)015<0193:eocapa>2.3.co;2},
	abstract = {We examined temporal and spatial patterns of feeding, distribution, and relative abundance of potential predators and competitors of juvenile kokanee and sockeye salmon Oncorhynchus nerka to determine which interactions might limit smolt production in Lake Ozette. Washington, Kokanee, the landlocked form of sockeye salmon, dominated the limnetic planktivore fish community, potentially creating greater intraspecific than interspecific competition. However, bioenergetic simulations and cladoceran egg-ratio analysis indicated that consumption demand by adult and juvenile kokanees and juvenile sockeye salmon could be satisfied by less than 1\% of the instantaneous production of the preferred large Daphnia pulicaria throughout the growing season. Juvenile kokanees and sockeye salmon represented 72\% of the annual diet by volume of large ( less than or equal to 300-mm fork length), limnetic northern squawfish Ptychocheilus oregonensis during winter-summer. Forty percent of the diet of large ( less than or equal to 300-mm fork length), limnetic cutthroat trout Oncorhynchus clarki consisted of age-0 and age-1 O. nerka (40-140 mm fork length) during spring and summer. The per-capita predation by cutthroat trout on juvenile salmonids was 25 times greater than predation by northern squawfish. Simulations indicated that 138,900 age-0 O. nerka (17.4\% of the estimated fry production) were eaten for every 1,000 large cutthroat trout in the lake, whereas only 5,600 age-0 fish (0.7\% of the fry production) were eaten per 1,000 large northern squawfish. Lower predation by northern squawfish was due to the small percentages (2-8\%) of these predators that inhabited the limnetic zone and interacted with age-0 age-1 O. nerka during spring and summer. We conclude that competitions is unlikely to limit O. nerka production, even with an intensive enhancement program. However, predation could undermine enhancement efforts if piscivore populations are sufficiently large; therefore, piscivore abundance must be assessed before total predation losses can be estimated.},
	pages = {193--207},
	number = {1},
	journaltitle = {North American Journal of Fisheries Management},
	author = {Beauchamp, David A. and Lariviere, Mark G. and Thomas, G. L.},
	date = {1995},
	file = {Beauchamp_et_al-1995-North_American_Journal_of_Fisheries_Management:C\:\\Users\\nh1087\\Zotero\\storage\\KKR7TRMT\\Beauchamp_et_al-1995-North_American_Journal_of_Fisheries_Management.pdf:application/pdf}
}

