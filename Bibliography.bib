	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.12568},
	keywords = {climate change, Camouflage, evolutionary adaptation, evolutionary rescue, fitness, molt phenology, natural selection, phenological mismatch, snow cover, snowshoe hare},
	file = {Snapshot:C\:\\Users\\nh1087\\Zotero\\storage\\HZCDM3P8\\ele.html:text/html;Zimova et al_2016_High fitness costs of climate change-induced camouflage mismatch.pdf:C\:\\Users\\nh1087\\Zotero\\storage\\8UPKX7N6\\Zimova et al_2016_High fitness costs of climate change-induced camouflage mismatch.pdf:application/pdf}
