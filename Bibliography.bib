	date = {2012},
	keywords = {Seabirds, Adaptations, Geese, Impacts, Large herbivores, Lemmings, Marine mammals, Mismatches, Parasites, Phenological changes, Plasticity, Polar bear, Range shifts, Rodents, Sea ice, Shorebirds, Threat, Trophic interactions, Tundra},
