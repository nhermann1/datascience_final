@article{berge_dark_2015,
	title = {In the dark: A review of ecosystem processes during the Arctic polar night},
	volume = {139},
	issn = {00796611},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0079661115001858},
	doi = {10.1016/j.pocean.2015.08.005},
	shorttitle = {In the dark},
	abstract = {Several recent lines of evidence indicate that the polar night is key to understanding Arctic marine ecosystems. First, the polar night is not a period void of biological activity even though primary production is close to zero, but is rather characterized by a number of processes and interactions yet to be fully understood, including unanticipated high levels of feeding and reproduction in a wide range of taxa and habitats. Second, as more knowledge emerges, it is evident that a coupled physical and biological perspective of the ecosystem will redeﬁne seasonality beyond the ‘‘calendar perspective”. Third, it appears that many organisms may exhibit endogenous rhythms that trigger ﬁtness-maximizing activities in the absence of light-based cues. Indeed a common adaptation appears to be the ability to utilize the dark season for reproduction. This and other processes are most likely adaptations to current environmental conditions and community and trophic structures of the ecosystem, and may have implications for how Arctic ecosystems can change under continued climatic warming.},
	pages = {258--271},
	journaltitle = {Progress in Oceanography},
	shortjournal = {Progress in Oceanography},
	author = {Berge, Jørgen and Renaud, Paul E. and Darnis, Gerald and Cottier, Finlo and Last, Kim and Gabrielsen, Tove M. and Johnsen, Geir and Seuthe, Lena and Weslawski, Jan Marcin and Leu, Eva and Moline, Mark and Nahrgang, Jasmine and Søreide, Janne E. and Varpe, Øystein and Lønne, Ole Jørgen and Daase, Malin and Falk-Petersen, Stig},
	urldate = {2021-05-11},
	date = {2015-12},
	langid = {english},
	file = {Berge et al. - 2015 - In the dark A review of ecosystem processes durin.pdf:C\:\\Users\\nh1087\\Zotero\\storage\\T3LDZ22S\\Berge et al. - 2015 - In the dark A review of ecosystem processes durin.pdf:application/pdf}
